{
  "name": "deepalert",
  "version": "2.0.0",
  "scripts": {
    "build": "make; tsc",
    "watch": "tsc -w",
    "test": "jest",
    "deploy": "make; tsc; cdk deploy",
    "cdk": "cdk",
    "prepare": "tsc",
    "install": "make"
  },
  "main": "./cdk/deepalert-stack.js",
  "type": "./cdk/deepalert-stack.d.ts",
  "files": [
    "Makefile",
    "go.*",
    "*.go",
    "internal",
    "lambda",
    "cdk"
  ],
  "devDependencies": {
    "@aws-cdk/assert": "1.75.0",
    "@types/jest": "^25.2.1",
    "@types/node": "10.17.5",
    "jest": "^25.5.0",
    "ts-jest": "^25.3.1",
    "aws-cdk": "1.75.0",
    "ts-node": "^8.1.0",
    "typescript": "~3.7.2"
  },
  "dependencies": {
    "@aws-cdk/aws-dynamodb": "1.75.0",
    "@aws-cdk/aws-iam": "1.75.0",
    "@aws-cdk/aws-lambda": "1.75.0",
    "@aws-cdk/aws-lambda-event-sources": "1.75.0",
    "@aws-cdk/aws-lambda-nodejs": "1.75.0",
    "@aws-cdk/aws-sns": "1.75.0",
    "@aws-cdk/aws-sns-subscriptions": "1.75.0",
    "@aws-cdk/aws-sqs": "1.75.0",
    "@aws-cdk/aws-stepfunctions": "1.75.0",
    "@aws-cdk/aws-stepfunctions-tasks": "1.75.0",
    "@aws-cdk/core": "1.75.0",
    "source-map-support": "^0.5.16"
  },
  "cdk-lambda": "/asset-output/index.js",
  "targets": {
    "cdk-lambda": {
      "context": "node",
      "includeNodeModules": {
        "aws-sdk": false
      },
      "sourceMap": false,
      "minify": false,
      "engines": {
        "node": ">= 12"
      }
    }
  }
}
